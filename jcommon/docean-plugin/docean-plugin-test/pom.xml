            <groupId>run.mone</groupId>
            <artifactId>docean-plugin-mvc</artifactId>
        </dependency>


        <dependency>
            <groupId>run.mone</groupId>
            <artifactId>docean-plugin-dmesh</artifactId>
        </dependency>


        <dependency>
            <groupId>run.mone</groupId>
            <artifactId>docean-plugin-sidecar</artifactId>
